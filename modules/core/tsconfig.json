{
  "compilerOptions": {
    "target": "ES5",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "React",
    "module": "system",
    "noImplicitAny": false,
    "lib": ["dom", "es5", "es6", "es2016", "es2017"],
    "noLib": false,
    "baseUrl": "./",
    "paths": {
      "rxjs/*": ["jspm_packages/npm/rxjs@5.0.3/*"]
    }
  }
}
