    "ejs": "^2.5.7",
    "sw-precache": "^5.2.0"
  },
  "devDependencies": {
    "@types/commander": "^2.9.1",
    "@types/ejs": "^2.3.33",
    "@types/npm": "^2.0.28",
    "typescript": "^2.4.2"
  }
}
